  displayResume: function() {
    console.log("----- Resume Summary -----");
    console.log("ğŸ‘¤ Name:", this.personalInfo.name);
    console.log("ğŸ“§ Email:", this.personalInfo.email);
    console.log("ğŸ“ Phone:", this.personalInfo.phone);
