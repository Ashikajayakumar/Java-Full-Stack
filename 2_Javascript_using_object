    console.log("ğŸ’¼ Experience:", this.experience.job1.role, "at", this.experience.job1.company);
    console.log("ğŸ› ï¸ Skills:", this.skills.programmingLanguages.join(", "));
    console.log("ğŸ¯ Hobbies:", this.hobbies.join(", "));
